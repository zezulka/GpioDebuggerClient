        <TextArea id="feedbackArea" fx:id="feedbackArea" editable="false" focusTraversable="false" layoutX="14.0" layoutY="14.0" opacity="0.45" prefHeight="143.0" prefWidth="389.0" wrapText="true" />
        <TextField id="ipValue" fx:id="ipValue" layoutX="140.0" layoutY="244.0" prefHeight="31.0" prefWidth="244.0" />
        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="14.0" layoutY="249.0" prefHeight="21.0" prefWidth="126.0" text="IP adress:" textAlignment="RIGHT">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Button layoutX="278.0" layoutY="297.0" mnemonicParsing="false" onKeyPressed="#keyTyped" onMouseClicked="#submitButtonPressed" prefHeight="42.0" prefWidth="106.0" text="Submit">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
    </children>
</Pane>
