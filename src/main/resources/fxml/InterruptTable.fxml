<Tab closable="false" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" text="Interrupts" fx:controller="layouts.controllers.InterruptTableController">
    <content>
        <AnchorPane minHeight="0.0" minWidth="0.0">
            <children>
                <ComboBox id="interruptTypeComboBox" fx:id="interruptTypeComboBox" prefHeight="25.0" prefWidth="200.0" layoutX="70.0" layoutY="14.0"/>
                <ComboBox id="pinComboBox" fx:id="pinComboBox" prefHeight="25.0" prefWidth="200.0" layoutX="300.0" layoutY="14.0"/>
                <Button id="addNewInterruptListenerButton" fx:id="addNewInterruptListenerButton" layoutX="525.0" layoutY="14.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="167.0" text="Add new listener" />
                <TableView id="tableView" fx:id="tableView" layoutX="8.0" layoutY="68.0" prefHeight="448.0" prefWidth="850.0">
                    <columns>
                        <TableColumn fx:id="pinName" editable="false" prefWidth="80.0" text="Pin Name" />
                        <TableColumn fx:id="interruptType" editable="false" prefWidth="80.0" text="Type" />
                        <TableColumn fx:id="timeAdded" editable="false" prefWidth="120.0" text="Time Added" />
                        <TableColumn fx:id="numberOfInterrupts" editable="false" prefWidth="120.0" text="Nr. Interrupts" />
                        <TableColumn fx:id="latestInterruptTime" editable="false" prefWidth="180.0" text="Latest" />
                        <TableColumn id="state" fx:id="state" prefWidth="150.0" text="Interrupt State" />
                    </columns>
                </TableView>
            </children>
        </AnchorPane>
    </content>
</Tab>
